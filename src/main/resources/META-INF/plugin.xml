<idea-plugin>
  <id>jones.foldtestblocks.fold-test-blocks</id>
  <name>Fold Test Blocks</name>
  <vendor>Gareth Jones</vendor>

  <depends>com.intellij.modules.lang</depends>
  <depends>org.jetbrains.plugins.ruby</depends>
  <depends>JavaScript</depends>

  <extensions defaultExtensionNs="com.intellij">
    <lang.foldingBuilder language="JSX Harmony" implementationClass="jones.foldtestblocks.JSTestBlockFoldingBuilder" />
    <lang.foldingBuilder language="JavaScript" implementationClass="jones.foldtestblocks.JSTestBlockFoldingBuilder" />
    <lang.foldingBuilder language="TypeScript" implementationClass="jones.foldtestblocks.JSTestBlockFoldingBuilder" />
    <lang.foldingBuilder language="TypeScript JSX" implementationClass="jones.foldtestblocks.JSTestBlockFoldingBuilder" />

    <lang.foldingBuilder language="ruby" implementationClass="jones.foldtestblocks.RubyTestBlockFoldingBuilder" />

    <projectService serviceImplementation="jones.foldtestblocks.BlockMatchersManager" />
  </extensions>

  <actions />
</idea-plugin>
